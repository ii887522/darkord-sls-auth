{
  "name": "darkord-sls-auth",
  "version": "1.0.0",
  "description": "Auth microservice that provides authentication and authorization to enhance the security of any web server",
  "scripts": {
    "deploy:stage": "cross-env AWS_SDK_LOAD_CONFIG=1 sls deploy --stage stage",
    "deploy:prod": "cross-env AWS_SDK_LOAD_CONFIG=1 sls deploy --stage prod --verbose",
    "deploy:api": "cross-env AWS_SDK_LOAD_CONFIG=1 sls auth-api:deploy --stage stage",
    "deploy:api:function": "cross-env AWS_SDK_LOAD_CONFIG=1 sls auth-api:deploy function --stage stage -f",
    "deploy:db": "cross-env AWS_SDK_LOAD_CONFIG=1 sls auth-db:deploy --stage stage",
    "deploy:funcs": "cross-env AWS_SDK_LOAD_CONFIG=1 sls auth-funcs:deploy --stage stage",
    "deploy:funcs:function": "cross-env AWS_SDK_LOAD_CONFIG=1 sls auth-funcs:deploy function --stage stage -f",
    "deploy:layer": "cross-env AWS_SDK_LOAD_CONFIG=1 sls auth-layer:deploy --stage stage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ii887522/darkord-sls-auth.git"
  },
  "author": "ii887522",
  "bugs": {
    "url": "https://github.com/ii887522/darkord-sls-auth/issues"
  },
  "homepage": "https://github.com/ii887522/darkord-sls-auth#readme",
  "devDependencies": {
    "@serverless/compose": "^1.3.0",
    "cross-env": "^7.0.3",
    "serverless": "^3.38.0",
    "serverless-better-credentials": "^2.0.0",
    "serverless-deployment-bucket": "^1.6.0",
    "serverless-iam-roles-per-function": "^3.2.0",
    "serverless-latest-layer-version": "^2.1.2",
    "serverless-prune-plugin": "^2.0.2"
  }
}
